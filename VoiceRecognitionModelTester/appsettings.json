{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Preselection": {
    "LSHMaxBucketSize": 100,
    "PhrasePairMinHeapSize": 10000,
    "PhrasesSetSize": 20,
    "ShinglePartCount": 3,
    "MinHashSignatureSize": 130,
    "LSHSimilarityThreshold": 0.5,
    "LSHAllowedFalseNegativeRate": 0.05
  }
}
